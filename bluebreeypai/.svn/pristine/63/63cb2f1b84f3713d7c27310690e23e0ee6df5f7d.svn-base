.p_select {
	position: relative;
	float: right;
	margin-right: 260px;
}

.p_select dt {
	height: 28px;
	display: inline-block;
	border: 1px solid #d2ccc4;
	background: #f2f2f2 url(../images/sanjiao_03.png) no-repeat 72px center;
	line-height: 28px;
	padding-left: 10px;
	cursor: pointer;
	width: 62px;
	padding-right: 12px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	position: relative;
	z-index: 99;
	border-radius: 4px;
	font-size: 12px;
}

.p_select dt:hover, .p_select dt.cur {
	border: 1px solid #409DFE;
	box-shadow: 0 0 3px #409DFE;
}

.p_select dd {
	position: absolute;
	left: 0;
	top: 29px;
	border: 1px solid #d2ccc4;
	background: #fff;
	display: none;
}

.p_select dd ul {
	padding: 4px;
	width: 76px;
	max-height: 250px;
	overflow: auto;
}

.p_select dd ul li a {
	line-height: 28px;
	display: block;
	padding: 0 8px;
	font-size: 12px;
}

.p_select dd ul li a:hover {
	background: #f5f5f5;
}

.p_rsetsex {
