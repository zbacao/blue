<script>
    $(".p_me>li").on("click", function () {
        $("*").removeClass("p_scolor");
        $(this).addClass("p_scolor");
    });


    $(".p_data>div").on("click", function () {
        $("*").removeClass("p_scolor");
        $(this).addClass("p_scolor");
    });

    //        切换个人设置
    $(".p_rsetb").on("click", "li", function () {
        $(this).addClass("p_rs").siblings().removeClass("p_rs");
    });


    $(function () {
        $(".p_select ").each(function () {
            var s = $(this);
            var z = parseInt(s.css("z-index"));
            var dt = $(this).children("dt");
            var dd = $(this).children("dd");
            var _show = function () {
                dd.slideDown(200);
                dt.addClass("cur");
                s.css("z-index", 900 + 1);
            };   //展开效果
            var _hide = function () {
                dd.slideUp(200);
                dt.removeClass("cur");
                s.css("z-index", 900);
            };    //关闭效果
            dt.click(function () {
                dd.is(":hidden") ? _show() : _hide();
            });
            dd.find("a").click(function () {
                dt.html($(this).html());
                _hide();
            });     //选择效果（如需要传值，可自定义参数，在此处返回对应的“value”值 ）
            $("body").click(function (i) {
                !$(i.target).parents(".p_select ").first().is(s) ? _hide() : "";
            });
        })
        $(".p_rsex ").each(function () {
            var s = $(this);
            var z = parseInt(s.css("z-index"));
            var dt = $(this).children("dt");
            var dd = $(this).children("dd");
            var _show = function () {
                dd.slideDown(200);
                dt.addClass("cur");
                s.css("z-index", 999 + 1);
            };   //展开效果
            var _hide = function () {
                dd.slideUp(200);
                dt.removeClass("cur");
                s.css("z-index", 999);
            };    //关闭效果
            dt.click(function () {
                dd.is(":hidden") ? _show() : _hide();
            });
            dd.find("a").click(function () {
                dt.html($(this).html());
                _hide();
            });     //选择效果（如需要传值，可自定义参数，在此处返回对应的“value”值 ）
            $("body").click(function (i) {
                !$(i.target).parents(".p_rsex ").first().is(s) ? _hide() : "";
            });
        })

    });

//            选择生日
