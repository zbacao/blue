.wrap{
    background-color: rgb(245,245,245);
}
/*整体内容框架*/
.p_contain{
	width: 1200px;
	height: 780px;
	background-color: rgb(245,245,245);
	margin: 0 auto;
	margin-top: 30px;
	margin-bottom: 160px;
}
.p_lc{
	float: left;
	width: 25%;
	height: 780px;
	background-color: #ffffff;
}
.p_rc{
	float: right;
	width: 875px;
	height: 780px;
	background-color: #ffffff;
}
.p_ltop{
	display: block;
	width: 300px;
	height: 120px;
}
.p_head{
	display: inline-block;
	width: 76px;
	height: 76px;
	background:url("../images/p_03.png") no-repeat center;
	margin-top: 40px;
	margin-left: 20px;

}

.p_headR{
	width: 180px;
	height: 120px;
	display: inline-block;
	vertical-align: top;
}
.p_name{
	font-size: 16px;
	display: inline-block;
	vertical-align: top;
	margin-top: 62px;
	margin-left: 25px;
	max-width: 100px;
	overflow: hidden;
	text-overflow: ellipsis;
	height: 20px;
	white-space: nowrap;
	font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
}
.p_sex{
	display: inline-block;
	width: 18px;
	height: 19px;
	margin-left: 15px;
	background: url("../images/pp_06.png") no-repeat center;
	margin-top: 62px;
}
.p_email{
	display: block;
	font-size: 14px;
	vertical-align: top;
	max-width: 150px;
	height: 20px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
	margin-left: 25px;
	margin-top: 3px;
}
.p_sign{
	display: block;
	margin-top: 30px;
	text-align: center;
}
.p_sign p{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
	font-size: 16px;
}
.p_csign{
	margin-top: 15px;
}
.p_csign a{
	display: inline-block;
	font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
	font-size: 14px;
	margin-left: 2px;
	color: #09ade2;
	line-height: 14px;
	vertical-align: top;
}
.p_pdata{
	display: block;
	width: 100%;
	height: 80px;
	background-color: #fff;
	margin-top: 40px;
	text-align: center;
}
.p_data{
	display: inline-block;
	width: 260px;
}
.p_data p{

}
.p_pdataa{
	display: inline-block;
	float: left;
	width: 80px;
	height: 80px;
	background-color: #f7f7f7;
	text-align: center;
	font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
	font-size: 14px;
	cursor: pointer;
}
.p_pdatab{
	display: inline-block;
	width: 80px;
	height: 80px;
	background-color: #f7f7f7;
	text-align: center;
