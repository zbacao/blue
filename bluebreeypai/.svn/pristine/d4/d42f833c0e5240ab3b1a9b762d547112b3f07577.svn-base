/*************游记板块  start **********/
.p-yj{
    width:100%;
}
.p-yj .note{
    float:left;
    width:278px;
    height:470px;
    margin-right:25px;
    border:1px solid #bababa;
    transition:all 0.4s;
    overflow:hidden;
}
.p-yj .note:hover{
    box-shadow:0 5px 20px rgba(0,0,0,0.2);
    transform: translateY(-2px);
}
.p-yj .note:nth-child(4){
    margin:0;
}
.p-yj .note a{
    width:100%;
    height:100%;
    border:1px solid #fff;
    display:block;
    position:relative;
}
.p-yj .note a img{
    height:380px;
    width:100%;
    display:block;
}
.p-yj .note a .note-title{
    font-size:16px;
    margin-top:15px;
    text-indent:1em;
}
.p-yj .note a .note-author{
    font-size:12px;
    color:#6dc7fa;
    margin-top:10px;
    text-indent:1em;
}

.p-yj .note a .note-desc{
    position:absolute;
    top:125px;
    left:0;
    width:86%;
    height:97px;
    background:rgba(0,0,0,0.5);
    padding:20px;
    /*display:none;*/
    margin-left:-280px;
    transition:all 0.4s;
}
.p-yj .note a:hover .note-desc{
    /*display:block;*/
    margin-left:0;
}
.p-yj .note a .note-desc .note-desc-cont{
    color:#fff;
    line-height:25px;
}

/*************游记板块  end **********/


