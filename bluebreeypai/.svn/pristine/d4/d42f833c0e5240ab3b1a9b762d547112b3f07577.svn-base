
/************************************轮播图 start********************************/
/*覆盖了swiper分页器  start*/
.swiper-slide{
    width: 100%;
}

.swiper-slide img{
    width: 100%;
    height:500px;
}

.swiper-container-horizontal>.swiper-pagination-bullets{
    text-align: center;
}
.p-yq .swiper-wrapper{
    margin-bottom:70px;
}
.p-ll .swiper-wrapper{
    margin-bottom:70px;
}
.p-yj .swiper-wrapper{
    margin-bottom:70px;
}
.swiper-container-horizontal>.swiper-pagination-bullets .swiper-pagination-bullet {
    margin: 0 4px;
}
.swiper-pagination-bullet {
    width: 15px;
    height: 15px;
    display: inline-block;
    border-radius: 100%;
    background: #a8e4c8;
    opacity: 1;
}
#swiper-pagination1 .swiper-pagination-bullet{
    background: #4e4e4e;
}
.swiper-pagination-bullet-active {
    background: #52c58c;
}
#swiper-pagination1 .swiper-pagination-bullet-active{
    background: #52c58c;
}
/* 覆盖了swiper分页器  end*/

/*轮播图上面的文字块  start*/
.swiper-container .banner-word{
    position:absolute;
    top:142px;
    left:212px;
    width:574px;
    height:182px;
    padding:24px 15px;
    background:rgba(0,0,0,0.5);
    z-index:9999;
}
.swiper-container .banner-word a{
    color:#fff;
}
.swiper-container .banner-word a h3{
    text-align:center;
    margin-bottom:24px;
    font-size:21px;
    font-weight:700;
}
.swiper-container .banner-word a p{
    font-size:14px;
    line-height:30px;
    text-indent:2em;
    letter-spacing: 1px;
}
/*轮播图上面的文字块  end*/

/************************************轮播图 end ********************************/

/************************************主产品展示 start********************************/
.main{
    width:1200px;
    min-width:1200px;
    margin:0 auto;
}
.product{
    border: 1px solid transparent;
}
/*板块标题及图标  start**********/
.product .p-title{
    width:140px;
    margin:40px auto 0;
    display:block;
    text-align:center;
    color: #292929;
}
.product .p-title .p-title-c{
    font-size:24px;
    display:block;
    margin-bottom:5px;
    position:relative;
}
.product .p-title .icon{
    text-align:center;
    position:relative;
}
/*图标左右的线条 start*/
.product .p-title .icon::before{
    content:"";
    display:block;
    width:48px;
    height:1px;
    position:absolute;
    left:0;
    top:8px;
    background:#e0e0e0;
}
.product .p-title .icon::after{
    content: "";
    display: block;
    width: 48px;
    height: 1px;
    position: absolute;
    right: -2px;
    top: 8px;
    background: #e0e0e0;
}
/*图标左右的线条 end*/

.product .p-title .icon img{
    margin-bottom:5px;
}
.product .p-title .p-title-e{
    font-size:14px;
    display:block;
    font-family: Arial;
    margin-bottom:5px;
    position:relative;
}
/*板块标题及图标  end**************/

/*******************产品展示里面的区块  start ***************/
.product .p-details{
    width:100%;
    /*height:500px;*/
    margin: 50px 0 20px 0;
}

/*************乐章板块  start ***********/
.p-yz .swiper-p-yz{
    width:100%;
    height:500px;
}
/*每一屏的通用设置  start*/
.swiper-p-yz .p-yz-cont{
    float:left;
    /*width:49.8%;*/
    height:208px;
    border:1px solid #fff;
    overflow:hidden;
}
.swiper-p-yz .p-yz-cont:nth-child(1){
    margin-bottom:20px;
}
.swiper-p-yz .p-yz-cont:nth-child(2){
    margin-bottom:20px;
}
/*每一屏的通用设置  end*/

/*左边图片 start*/
.swiper-p-yz .p-yz-cont .p-yz-cont-L{
    float:left;
    width:210px;
    height:210px;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-L img{
    width:210px;
    height:210px;
}
/*左边图片 end*/
/*右边内容  start*/
.swiper-p-yz .p-yz-cont .p-yz-cont-R{
    float:right;
    height:186px;
    position:relative;
    width:336px;
    padding:12px 22px;
}
.artical-title:hover{
    color:#52c58c;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .artical-title{
    display:block;
    font-size:16px;
    font-weight:700;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .author{
    display:block;
    color:#6dc7fa;
    font-size:12px;
    margin:12px 0;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R p{
    color: #646464;
    line-height: 24px;
    letter-spacing: 1px;
    overflow:hidden;
    height: 100px;
}
 .time{                                                                /*此样式可以共用*/
    color:#ccc;
    position: absolute;
    top: 44px;
    left: 28%;
}
.time i.time-icon{
    width:20px;
    height:20px;
    position:absolute;
    top: -1px;
    left: -20px;
    background:url(../indexImg/clock_03.png) no-repeat center center;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .zan{
    color:#ccc;
    position: absolute;
    top: 180px;
    right: 25px;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .zan i.zan-num{
    margin-right:20px;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .zan i.zan-icon{
    width:20px;
    height:20px;
    position:absolute;
    top:0;
    left:-18px;
    background:url(../indexImg/zan_05.png) no-repeat center center;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .zan i.pl-icon{
    width:20px;
    height:20px;
    position:absolute;
    top:0;
    left:20px;
    background:url(../indexImg/pinglun_05.png) no-repeat center center;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .zan i.zan-icon:hover{
    cursor:pointer;
}
.swiper-p-yz .p-yz-cont .p-yz-cont-R .zan i.pl-icon:hover{
    cursor:pointer;
}
/*右边内容  end*/
/*************乐章板块  end **********/


/*************听说板块  start **********/
.p-ts{
    width:100%;
    background:#f5f5f5;
}
/*横向的专辑  start*/
.p-ts .horizontal{
    float:left;
    width:80%;
    height:100%;
}
.p-ts .horizontal .p-album{
    float:left;
    width:50%;
    height:50%;
}
/*专辑图片 start */
.p-ts .horizontal .p-album .album-pic{
    float:left;
    width:50%;
    position:relative;
}
.p-ts .horizontal .p-album .album-pic img{
    width:100%;
    height:250px;
}
/*专辑图片 end */

/*专辑介绍 start */
/*专辑介绍公共样式*/
.p-ts .p-album .album-introduce{
    width: 40%;
    padding: 20px 20px;
}
/*横向专辑样式*/
.p-ts .horizontal .p-album .album-introduce{
    float: left;
    margin-left: 8px;
}
/*专辑歌手,专辑名,描述公共样式*/
.p-ts .p-album .album-introduce .singer{
    color:#52ca91;
    font-size:12px;
    margin-bottom:10px;
}
.p-ts .p-album .album-introduce .album-name{
    font-size:16px;
    margin-bottom:15px;
}
.p-ts .p-album .album-introduce .album-desc{
    font-size: 14px;
    color: #666;
    /* line-height: 16px; */
    line-height: 1.2em;
    letter-spacing: 2px;
    overflow: hidden;
    height: 115px;
}
/*专辑介绍 end */
/*横向的专辑  end*/

/*竖向的专辑  start*/
.p-ts .vertical{
    float:right;
    width:20%;
    height:100%;
}
.p-ts .vertical .p-album{
    width:100%;
    height:100%;
}
.p-ts .vertical .p-album .album-pic{
    position:relative;
}
.p-ts .vertical .p-album .album-pic img{
    width:100%;
    height:250px;
}
.p-ts .vertical .p-album .album-introduce{
    width:85%;
}
/*竖向的专辑  end*/

/*专辑里面的小三角 start*/
.p-ts .horizontal .p-album .album-pic .to-left::after{
    content:"";
    display:block;
    width:0;
    height:0;
    position:absolute;
    top:50px;
    right:0;
    border-top:11px solid transparent;
    border-right:11px solid #fff;
    border-bottom:11px solid transparent;
    border-left:11px solid transparent;
}
.p-ts .horizontal .p-album .album-pic .to-right::after{
    content:"";
    display:block;
    width:0;
    height:0;
    position:absolute;
    top:50px;
    left:0;
    border-top:11px solid transparent;
    border-right:11px solid transparent;
    border-bottom:11px solid transparent;
    border-left:11px solid #fff;
}
.p-ts .vertical .p-album .album-pic .to-up::after{
    content:"";
    display:block;
    width:0;
    height:0;
    position:absolute;
    bottom: 4px;
    left: 115px;
    border-top:11px solid transparent;
    border-right:11px solid transparent;
    border-bottom:11px solid #fff;
    border-left:11px solid transparent;
}
/*专辑里面的小三角 end*/
/*************听说板块  end **********/

