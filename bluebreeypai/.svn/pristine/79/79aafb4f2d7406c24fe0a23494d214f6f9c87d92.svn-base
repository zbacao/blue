.i_markCup{
    width: 1200px;
    height: 7200px;
    min-width: 1200px;
    background-color: #ffffff;
    margin: 0 auto;
}
.i_cbody{
    width: 100%;
    height: 6700px;
    background: #f2f2f2;
}
.i_cupTitle{
    width: 750px;
    height: 180px;
    margin: 0 auto;
    text-align: center ;
}

.i_cupTitle p{
    font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
    font-size: 26px;
    color: #109d59;
}

.i_cta{
    display: block;
    padding-top: 40px;
}
.i_ctb{
    padding-top: 10px;
}
.i_ctc{
    display: inline-block;
    padding-top: 10px;
    margin-left: 100px;
}
.i_cbuy{
    display: inline-block;
    width: 115px;
    height: 30px;
    background-color: #ff4400;
    float: right;
    margin-top: 15px;
    border-radius: 4px;
    text-align: center;
    cursor: pointer;
}
.i_cbuy a{
    font-family:"Microsoft YaHei", Arial, Helvetica, sans-serif;
    font-size: 18px;
    line-height: 30px;
    color: #ffffff;
    letter-spacing:4px;
    padding-left: 2px;
}
.i_cupImg{
    width: 750px;
    height: 100%;
    margin: 0 auto;
}
.i_text{
    width: 800px;
    height: 120px;
    background-color: #f2f2f2;
    border-radius: 4px;
}
.i_setText{
    width:100px;
    height: 40px;
    background-color: #f68344;
    float: right;
    margin-right: -50px;
    margin-top: 10px;
    text-align: center;
    border-radius: 4px;
}
.i_setText a{
    font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
    font-size: 16px;
    line-height: 40px;
    letter-spacing: 5px;
    color: #fdede6;
    padding-left: 5px;
}